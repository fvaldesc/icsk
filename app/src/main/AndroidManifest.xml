<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.icsk.comandaoperadorv5">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComandaOperadorV5"
        android:usesCleartextTraffic="true">
        <activity android:name=".actividadActividadOperador"></activity>
        <activity android:name=".ingresarActividad" />
        <activity android:name=".actividadJornada" />
        <activity android:name=".actividadSistemas" />
        <activity android:name=".actividadTurno"/>
        <activity android:name=".ingresoUsuario" />
        <activity android:name=".presentacionActividad">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>